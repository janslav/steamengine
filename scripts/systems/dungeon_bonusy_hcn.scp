[itemdef i_hcn_padaciDvere]
name=padaci dvere
id=06f5
type=t_normal
flip=1
dupelist=06F6

on=@Create
attr=010
moreZ=120

on=@Timer
if (<eval more1> < 9) && (<eval moreX> == 0)
  f_moveUp
  if (<more1> == 0)
    sfx(557)
  endif
  timerD=2
  more1=<more1> +1
elseif (<eval more1> == 9) && (<eval moreX> == 0)
  timer=<eval moreZ>
  more2=9
  moreX=1
elseif (<eval more2> > 0)
  f_moveDown
  if (<more2> == 9)
    sfx(557)
  endif
  timerD=2
  more2=<more2> -1
else	//dvere dopadly na zem
  more1=0
  more2=0
  moreX=0
  if (<link>)
    link.f_hcn_vaseSwitcher_reset
  endif
endif
return 1

[function f_moveUp]
z=<z>+2
[function f_moveDown]
z=<z>-2

[function f_hcn_padaciDvere_shut]
if <eval more2>
  moreX=1
  timer=0
elseif <eval more1>
  more2=<eval more1>
  moreX=1
  timer=0
endif

[function f_hcn_padaciDvere_open]
if (<eval more1> < 9)
  moreX=0
  timer=0
elseif (<eval more2> < 9)
  more1=<eval more2>
  moreX=0
  timer=0
endif

