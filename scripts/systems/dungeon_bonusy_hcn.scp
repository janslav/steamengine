  if (dispID==03c8)
    sfx(04cf)
    dispID=03c5
    src.sysMessage("A vida! Zed povolila a zjevne v sobe neco skryva.")
    update
  endif
  more1=0
  timer=3600
else
  dispID=03c8
  update
endif
