[events e_healer]
on=@userclick
if (src.tag(realm)<0)
 src.sysmessage("Jsi stizen kletbou bohu a vyvrzen z tohoto sveta. Nemohu te navratit k zivotu.")
 return 1
endif
if (src.flag_dead)
	if (distance<5)
  		src.dospell_final(59,1000)
  	endif
endif

[speech spk_healer]
on=*res*
if (src.tag(realm)<0)
 src.sysmessage("Jsi stizen kletbou bohu a vyvrzen z tohoto sveta. Nemohu te navratit k zivotu.")
 return 1
endif
if (src.flag_dead)
  if (srccanseelos)
    src.dospell_final(59,1000)
  endif
endif
