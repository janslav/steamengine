[itemdef i_nofame]
id=i_gold
name=nofame
on=@create
attr_invis=1

[itemdef i_poresu]
id=i_gold
name=poresu
on=@create
attr_invis=1

[itemdef i_nores]
id=i_gold
name=nores
on=@create
attr_invis=1

[itemdef i_monster_create]
id=i_memory
type=t_eq_script
name=created memory

on=@equip
src.tag(combattype,"npc")
timer=0

on=@timer
src=<cont>
cont.creaturestart
remove
return 1

[function creaturestart]
events e_gethit
events e_spelleffect

if (body==c_spider_giant)
  events +e_removesilk
endif

if (tag(npc_flee))
 events=+e_npc_flee
endif

if (tag(weapflag))
elseif (zbran_char)
 if (tag(weaponuid).type==t_weapon_sword)
  tag(atskill,40)
  tag(weapflag,dam_physical|dam_sharp|dam_slashing)
 elseif (tag(weaponuid).type==t_weapon_fence)
  tag(atskill,42)
  tag(weapflag,dam_physical|dam_sharp|dam_piercing)
 elseif (tag(weaponuid).type==T_WEAPON_MACE_CROOK)||(tag(weaponuid).type==T_WEAPON_MACE_SMITH)||(tag(weaponuid).type==T_WEAPON_MACE_STAFF)
  tag(atskill,41)
  tag(weapflag,dam_physical|dam_blunt)
 elseif (tag(weaponuid).type==t_weapon_bow_run)||(tag(weaponuid).type==T_WEAPON_XBOW)||(tag(weaponuid).type==T_WEAPON_BOW)
  tag(weapflag,dam_physical|dam_piercing)
  tag(atskill,31)
 else
  tag(weapflag,dam_physical|dam_blunt)
  tag(atskill,43)//wrest
 endif
elseif !(tag(weapflag))
 tag(weapflag,dam_physical|dam_blunt)
 tag(atskill,43)//wrest
endif

if (tag(atskill))
else
 tag(atskill,43)
endif

if (hiding)
  skill hiding
endif
if(tag(nores))&&(flag_spawned)
 tag.remove(nores)
 newitem(i_nores)
 lastnew.bounce
endif

if !(maxhits)
 maxhits=<str>
 hits=<maxhits>
endif
if !(maxstam)
 maxstam=<dex>
endif
if !(tag(npc_speed))//aby monstra mela dost staminy - enrico
 tag(npc_speed,<maxstam>)
 arg(max_stam,<eval <maxstam>*5>)
 if (arg(max_stam) > 32767)
  maxstam=32767
 else 
  maxstam=<arg(max_stam)>
 endif
endif
stamina=<maxstam>//az sem - enrico
//tag(additdam,<eval ({800 1200}*<avervalue(<typedef.dam>)>)/2000>)
//spawn:
if (flag_spawned)
 memoryfindtype(MEMORY_ISPAWNED).link.events t_allspawns
 memoryfindtype(MEMORY_ISPAWNED).link.tag(lastspawned,<uid>)
 tag(lecitel,<memoryfindtype(MEMORY_ISPAWNED).link.tag.lecitel>)
 if (tag(lecitel))
  newequip(i_friendslayer)
 endif
 tag(summoner,<memoryfindtype(MEMORY_ISPAWNED).link.tag.summoner>)
 //if (<safe.tag.summoner>)
 // newequip i_corpselayer
 //endif

 //pro guardovaci a bossovy spawny: 
 if(memoryfindtype(MEMORY_ISPAWNED).link.tag(myboss)) //spawngem je guard nejakeho bosse  
  arg(gem,<memoryfindtype(MEMORY_ISPAWNED).link>)
  arg(bossgem,<memoryfindtype(MEMORY_ISPAWNED).link.tag(myboss)>)
  if(bossgem.tag(lastspawned))
   if(bossgem.tag(lastspawned).ischar)
    makeguard(<bossgem.tag(lastspawned)>) //nove monstrum bude hlidat sveho bosse
    bossgem.tag(lastspawned).makeboss(<uid>) //jsem boss tohoto monstra
   endif
  endif  
 endif
 if(memoryfindtype(MEMORY_ISPAWNED).link.tag(guardscount)) //narodil se novy boss  
  arg(bossgem,<memoryfindtype(MEMORY_ISPAWNED).link>)
  arg(i,1)
  while(i <= <bossgem.tag(guardscount)>)
   arg(guardsgem,<bossgem.tag(guardedby_<eval i>)>)
   if(guardsgem.tag(lastspawned))
    if(guardsgem.tag(lastspawned).ischar) //zijici potencialni strazce
     guardsgem.tag(lastspawned).makeguard(<uid>) //guard bude hlidat sveho bosse
     makeboss(<guardsgem.tag(lastspawned)>) //nove monstrum bude bossem sveho strazce
    endif
   endif
   arg(i,#+1)
  endwhile
 endif
endif
//dat vedet o svem vzniku okolnim lecitelum
sector.allchars(f_addfriend)
//fame add:
finduid(#01A).say(<memoryfindtype(0200).link.tag(spawnevent1)>)
if (safe.memoryfindtype(0200).link.tag(negateOnDeath))
  events=+e_onDeath_negateSpawn
endif
if memoryfindtype(0200).link.tag(spawnevent1)
  events <memoryfindtype(0200).link.tag(spawnevent1)>
endif
if memoryfindtype(0200).link.tag(spawnevent2)
  events <memoryfindtype(0200).link.tag(spawnevent2)>
endif
if memoryfindtype(0200).link.tag(spawnevent3)
  events <memoryfindtype(0200).link.tag(spawnevent3)>
endif
if memoryfindtype(0200).link.tag(removeAfterTimer)
  newequip(i_zmizni)
  lastnew.timer=<memoryfindtype(0200).link.tag(removeAfterTimer)>
endif
if (tag(thiefmrcha)==1)
 flag_insubstantial=1
endif
if memoryfindtype(0200).link.tag(firstspawn)
Flag_Conjured=1
endif
if (findid(i_nofame))
 tag(experience,0)
 findid(i_nofame).remove
else
 if (findid(i_gold))
  findid(i_gold).amount=<eval (<eval <tag.experience>>*900)/1000>
  if (rescount(i_gold)>{30 100000})
   giveitem(i_mi_bag)
  endif
  if (rescount(i_gold)>{3500 100000})&&({1 3} == 1)
   giveitem(i_coal_elemental)
  endif
  if (<body>==c_elem_fire_m) || (<color>==044c) || (<obody>==c_fire_fanatik) || (<obody>==c_fire_lord)
  //firenpc,elmove ohne,verbatimovi fireanti
   if (rescount(i_gold)>{20 30000})
    giveitem(i_coal_elemental)
   endif
  endif
  if (rescount(i_gold)>{3500 100000})&&({1 10} == 1)
   giveitem(i_krystal_eben)
  endif
  if (rescount(i_gold)>{3500 100000})&&({1 8} == 1)
   giveitem(i_tetiva_eben)
  endif
  if (rescount(i_gold)>{30 100000})&&({1 20} == 5)
   giveitem(i_u_treasure_map)
  endif
 endif
 if (tag(experience)<10)
  tag(experience,10)
 endif
endif

if (flag_conjured)
 if (memoryfindtype(MEMORY_IPET))
  arg(owner,<memoryfindtype(MEMORY_IPET).link>)
  if (finduid(<arg(owner)>).isplayer)
    newitemsafe(i_rune_hassummon)
    tag(runecaster,<lastnew>)
    lastnew.link=<uid>
    lastnew.cont=<owner>
    if (findid(i_rune_summon_creature))
      newitemsafe(i_summonremove)
      equip(lastnew)
      lastnew.timer=<findid(i_rune_summon_creature).timer>-1
    endif
  endif
 endif
endif
return 1

[function f_addfriend]
if (findlayer(80))
  if (ismyfriend)
    newitemsafe(i_relating_crystal)
    lastnew.link=<src>
    lastnew.dispid=<def.icon>
    lastnew.name=<src.name>
    lastnew.cont=<findlayer(80)>  
  endif
endif

[itemdef i_summonremove]
id=i_rune_summon_creature
type=t_eq_script
name=summoncreaturetimer

on=@timer
cont.remove
remove
return 1

[events e_removesilk]
on=@itemstep
if (act.baseid==i_reag_spider_silk)
 act.more=0
 act.attr=0
 act.bounce
endif

[itemdef i_rune_hassummon]
NAME=summon timer
ID=i_rune_summon_creature
TYPE=T_EQ_SCRIPT 

on=@create
timer=5

on=@unequip
if (link)
 if (link.issumon)
  link.remove
 else
  link.cont.dismount
 endif
endif

on=@timer
if (link)
 if (cont.rescount(i_rune_hassummon)>eval((cont.tag(level)/5)+1))||(cont.rescount(i_rune_hassummon)>5)
   remove
 endif
else
 remove
endif
timer=5
return 1

[events e_monster]
on=@Death //kun smazat
findlayer(25).remove

   
on=@npcseenewplayer
if(<tag(karma)>)
 karma=<tag(karma)> //oprava pro pripad predchoziho videni mystika
endif
if (flag_spawned)//oprava bugnuti Z
 z=<memoryfindtype(MEMORY_ISPAWNED).link.z>
 fix
endif
if (<src.tag(hypno_mystik)>) 
 if(<karma> < 0)
  tag(karma,<karma>) 
  karma=0 //jen takhle nebude utocit na mystika 
  tag(na_mystika,1) //na mystikovu figurinu utocit muze
 endif
 //say(vidim mystika <src.tag(name_m)>) 
 return 1 //zapomen
endif

