[ITEMDEF i_gq001_strom]
ID=0cdd
NAME=Mrtvy strom
TYPE=t_normal

CATEGORY=Moria Questy
SUBSECTION=gq001
DESCRIPTION=Mrtvy strom

on=@create 
attr=attr_move_never
color=0759
more1=2 //pocet kolik kousku kury bude na strome


ON=@userCLICK
src.sysmessage Davno mrtvy strom s pokroucenymi vetvemi a zcernalou kurou

on=@userdclick 
if (tag(tmr)< serv.time) 
more1=2
endif
if (<src.tag(gq001)>==1)
 if(<more1> > 0) 
 //snizit citac 
 more1 =<eval <more1>-1> 
 tag(tmr,<eval serv.time+60*90>) 
 src.giveitem(i_gq001_kura) 
 //snizil-li se citac uz na 0 
 else 
 src.sysmessage(Tady uz zadna pouzitelna kura neni) 
 endif 
else
 src.sysmessage(Kura stromu se pri dotyku droli) 
endif
return 1


