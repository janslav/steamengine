[function f_questMenu_hasRelevantTag]
arg(tagCount,<argvcount>-3)
arg(tagNumber,3)
arg(cycleArg,0)
if (<argv(2)>)
  while (<cycleArg> < <tagCount>)
    if (0<src.qtag(<argv(<tagNumber>)>)> !=0)
      return 1
    else
      arg(tagNumber,#+1)
      arg(cycleArg,#+1)
    endif
  endwhile
else
  while (<cycleArg> < <tagCount>)
    if (0<src.tag(<argv(<tagNumber>)>)> !=0)
      return 1
    else
      arg(tagNumber,#+1)
      arg(cycleArg,#+1)
    endif
  endwhile
endif
return 0



